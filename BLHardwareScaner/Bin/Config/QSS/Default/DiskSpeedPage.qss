QComboBox#comboBoxSeqSize
{
    border: 1px solid gray;
}

QComboBox#comboBoxSeqSize::drop-down
{       
    border-image: url(:/ControlImage/DropDown.png);
}

QComboBox#comboBoxSeqSize QAbstractItemView
{
    border: 1px solid gray;
    outline: 0px;
}

